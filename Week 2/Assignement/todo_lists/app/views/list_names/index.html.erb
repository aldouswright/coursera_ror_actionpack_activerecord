<p id="notice"><%= notice %></p>

<h1>Listing List Names</h1>

<table>
  <thead>
    <tr>
      <th>List due date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @list_names.each do |list_name| %>
      <tr>
        <td><%= list_name.list_due_date %></td>
        <td><%= link_to 'Show', list_name %></td>
        <td><%= link_to 'Edit', edit_list_name_path(list_name) %></td>
        <td><%= link_to 'Destroy', list_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List name', new_list_name_path %>
